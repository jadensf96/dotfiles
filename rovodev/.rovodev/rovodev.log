n\n        local branch=$(cd \"$project_path\" && git branch --show-current 2>/dev/null)\n        if [[ -n \"$branch\" ]]; then\n            git_info=\" (git:$branch)\"\n        fi\n    fi\n    \n    # Get project type info\n    if [[ -f \"$project_path/package.json\" ]]; then\n        extra_info=\" [Node.js]\"\n    elif [[ -f \"$project_path/Cargo.toml\" ]]; then\n        extra_info=\" [Rust]\"\n    elif [[ -f \"$project_path/go.mod\" ]]; then\n        extra_info=\" [Go]\"\n    elif [[ -f \"$project_path/Makefile\" ]]; then\n        extra_info=\" [Make]\"\n    elif [[ -f \"$project_path/CMakeLists.txt\" ]]; then\n        extra_info=\" [CMake]\"\n    fi\n    \n    echo \"$project_name$git_info$extra_info | $project_path\"\n}\n\n# Function to switch to project\nswitch_to_project() {\n    local project_path=\"$1\"\n    \n    if [[ ! -d \"$project_path\" ]]; then\n        echo \"Error: Directory $project_path does not exist\"\n        return 1\n    fi\n    \n    # Save to recent projects\n    echo \"$project_path\" >> \"$PROJECT_SWITCHER_CONFIG\"\n    \n    # Keep only last 20 recent projects\n    tail -20 \"$PROJECT_SWITCHER_CONFIG\" > \"${PROJECT_SWITCHER_CONFIG}.tmp\" && mv \"${PROJECT_SWITCHER_CONFIG}.tmp\" \"$PROJECT_SWITCHER_CONFIG\"\n    \n    # Change directory\n    cd \"$project_path\" || return 1\n    \n    echo \"📁 Switched to: $(basename \"$project_path\")\"\n    echo \"📍 Path: $project_path\"\n    \n    # Show project info\n    if [[ -d \".git\" ]]; then\n        echo \"🌿 Git branch: $(git branch --show-current 2>/dev/null || echo 'unknown')\"\n        echo \"📊 Git status:\"\n        git status --porcelain | head -5\n    fi\n    \n    # Show directory contents\n    echo \"\"\n    echo \"📋 Contents:\"\n    if command -v exa >/dev/null 2>&1; then\n        exa -la --icons --git | head -10\n    else\n        ls -la | head -10\n    fi\n}\n\n# Main function\nmain() {\n    local search_term=\"$1\"\n    \n    # Get all projects\n    local projects=($(find_projects))\n    \n    if [[ ${#projects[@]} -eq 0 ]]; then\n        echo \"No projects found!\"\n        return 1\n    fi\n    \n    # If search term provided, filter projects\n    if [[ -n \"$search_term\" ]]; then\n        local filtered_projects=()\n        for project in \"${projects[@]}\"; do\n            if [[ \"$project\" == *\"$search_term\"* ]]; then\n                filtered_projects+=(\"$project\")\n            fi\n        done\n        projects=(\"${filtered_projects[@]}\")\n    fi\n    \n    # If only one project matches, switch directly\n    if [[ ${#projects[@]} -eq 1 ]]; then\n        switch_to_project \"${projects[0]}\"\n        return $?\n    fi\n    \n    # Use fzf for selection\n    local project_list=\"\"\n    for project in \"${projects[@]}\"; do\n        project_list+=\"$(get_project_info \"$project\")\"$'\\n'\n    done\n    \n    local selected=$(echo \"$project_list\" | fzf \\\n        --height=60% \\\n        --layout=reverse \\\n        --border \\\n        --preview='echo {} | cut -d\"|\" -f2 | xargs ls -la' \\\n        --preview-window=right:50% \\\n        --prompt=\"🔍 Select project: \" \\\n        --header=\"Use ↑↓ to navigate, Enter to select, Esc to cancel\")\n    \n    if [[ -n \"$selected\" ]]; then\n        local project_path=$(echo \"$selected\" | cut -d'|' -f2 | xargs)\n        switch_to_project \"$project_path\"\n    fi\n}\n\n# Run main function with all arguments\nmain \"$@\""}
