s\n    case \"$1\" in\n        \"-h\"|\"--help\")\n            show_help\n            return 0\n            ;;\n        \"-l\"|\"--list\")\n            list_sessions\n            return 0\n            ;;\n        \"-k\"|\"--kill\")\n            if [[ -n \"$2\" ]]; then\n                kill_session \"$2\"\n            else\n                interactive_select \"kill\"\n            fi\n            return 0\n            ;;\n        \"-c\"|\"--create\")\n            if [[ -n \"$2\" ]]; then\n                create_session \"$2\" \"$PWD\"\n                if [[ $? -eq 0 ]]; then\n                    switch_to_session \"$2\"\n                fi\n            else\n                echo \"❌ Please provide a session name\"\n                return 1\n            fi\n            return 0\n            ;;\n        \"-n\"|\"--new\")\n            create_from_pwd\n            return 0\n            ;;\n        \"\")\n            # No arguments - interactive mode\n            interactive_select \"switch\"\n            return 0\n            ;;\n        *)\n            # Session name provided\n            local session_name=\"$1\"\n            if tmux has-session -t \"$session_name\" 2>/dev/null; then\n                switch_to_session \"$session_name\"\n            else\n                echo \"🤔 Session '$session_name' doesn't exist. Creating it...\"\n                create_session \"$session_name\" \"$PWD\"\n                if [[ $? -eq 0 ]]; then\n                    switch_to_session \"$session_name\"\n                fi\n            fi\n            ;;\n    esac\n}\n\n# Run main function with all arguments\nmain \"$@\""}
