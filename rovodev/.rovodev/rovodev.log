   tmux select-layout -t \"$session_name\" \"$DEFAULT_LAYOUT\"\n    \n    # If it's a git repository, create useful windows\n    if [[ -d \"$start_dir/.git\" ]]; then\n        # Create a git window\n        tmux new-window -t \"$session_name\" -n \"git\" -c \"$start_dir\"\n        tmux send-keys -t \"$session_name:git\" \"lazygit\" C-m\n        \n        # Create an editor window\n        tmux new-window -t \"$session_name\" -n \"editor\" -c \"$start_dir\"\n        tmux send-keys -t \"$session_name:editor\" \"vim .\" C-m\n        \n        # Go back to first window\n        tmux select-window -t \"$session_name:1\"\n    fi\n    \n    # If it's a project directory, create development windows\n    if [[ -f \"$start_dir/package.json\" ]]; then\n        tmux new-window -t \"$session_name\" -n \"dev\" -c \"$start_dir\"\n        tmux send-keys -t \"$session_name:dev\" \"npm run dev\" C-m\n    elif [[ -f \"$start_dir/Cargo.toml\" ]]; then\n        tmux new-window -t \"$session_name\" -n \"dev\" -c \"$start_dir\"\n        tmux send-keys -t \"$session_name:dev\" \"cargo watch -x run\" C-m\n    elif [[ -f \"$start_dir/go.mod\" ]]; then\n        tmux new-window -t \"$session_name\" -n \"dev\" -c \"$start_dir\"\n        tmux send-keys -t \"$session_name:dev\" \"go run .\" C-m\n    fi\n    \n    # Save session info\n    echo \"$session_name|$start_dir|$(date +%s)\" >> \"$TMUX_SESSION_CONFIG\"\n    \n    echo \"✅ Session '$session_name' created successfully!\"\n}\n\n# Function to switch to a session\nswitch_to_session() {\n    local session_name=\"$1\"\n    \n    if ! tmux has-session -t \"$session_name\" 2>/dev/null; then\n        echo \"❌ Session '$session_name' does not exist!\"\n        return 1\n    fi\n    \n    # If we're already in tmux, switch session\n    if [[ -n \"$TMUX\" ]]; then\n        tmux switch-client -t \"$session_name\"\n    else\n        # If not in tmux, attach to session\n        tmux attach-session -t \"$session_name\"\n    fi\n}\n\n# Function to kill a session\nkill_session() {\n    local session_name=\"$1\"\n    \n    if ! tmux has-session -t \"$session_name\" 2>/dev/null; then\n        echo \"❌ Session '$session_name' does not exist!\"\n        return 1\n    fi\n    \n    tmux kill-session -t \"$session_name\"\n    echo \"🗑️  Session '$session_name' killed\"\n    \n    # Remove from config\n    if [[ -f \"$TMUX_SESSION_CONFIG\" ]]; then\n        grep -v \"^$session_name|\" \"$TMUX_SESSION_CONFIG\" > \"${TMUX_SESSION_CONFIG}.tmp\" && mv \"${TMUX_SESSION_CONFIG}.tmp\" \"$TMUX_SESSION_CONFIG\"\n    fi\n}\n\n# Function to list all sessions with details\nlist_sessions() {\n    local sessions=($(get_sessions))\n    \n    if [[ ${#sessions[@]} -eq 0 ]]; then\n        echo \"No tmux sessions found.\"\n        return 0\n    fi\n    \n    echo \"📋 Active tmux sessions:\"\n    echo \"========================\"\n    \n    for session in \"${sessions[@]}\"; do\n        echo \"$(get_session_info \"$session\")\"\n    done\n}\n\n# Function for interactive session selection\ninteractive_select() {\n    local action=\"$1\"\n    local sessions=($(get_sessions))\n    \n    if [[ ${#sessions[@]} -eq 0 ]]; then\n        echo \"No tmux sessions found.\"\n        return 1\n    fi\n    \n    # Build session list for fzf\n    local session_list=\"\"\n    for session in \"${sessions[@]}\"; do\n        session_list+=\"$(get_session_info \"$session\")\"$'\\n'\n    done\n    \n    local prompt=\"🔍 Select session\"\n    case \"$action\" in\n        \"kill\") prompt=\"🗑️  Select session to kill\" ;;\n        \"switch\") prompt=\"🔄 Select session to switch to\" ;;\n    esac\n    \n    local selected=$(echo \"$session_list\" | fzf \\\n        --height=60% \\\n        --layout=reverse \\\n        --border \\\n        --preview='echo {} | cut -d\"|\" -f1 | xargs tmux list-windows -t 2>/dev/null || echo \"No windows found\"' \\\n        --preview-window=right:50% \\\n        --prompt=\"$prompt: \" \\\n        --header=\"Use ↑↓ to navigate, Enter to 